
.nav{
  height: 69px;
  border-bottom: 3px solid #e7e7e7;
}
.nav .category-menu{
  height: 68px;
}
.nav .left-logo img{
  margin-top: 40px;
  margin-left: 170px;
}
.nav .right-list p:first-child{
  font-size: 24px;
  color: #aaaaaa;
  position: absolute;
  top: 50px;
  left: 400px;
}
.nav .right-list p:last-child{
  font-size: 24px;
  color: #aaaaaa;
  position: absolute;
  top: 50px;
  right: 150px;
}
.nav .right-list p:last-child span{
  color: #333;
}
.register{
	width: 500px;
	height: 500px;
	margin-top: 50px;
}
.error{
	text-indent: 10px;
	color: red;
}
.register h1{
	width: 499px;
	height: 30px;
	font-size: 18px;
	color: #6c6c6c;
	border: 1px solid blue;
	background-color: #ecf5ff;
}
.register #registerForm{
	width: 499px;
	height: 400px;
	border: 1px solid blue;
}
.register form .active{
	width: 400px;
	height: 50px;
	margin-top: 20px;
	margin-left: 50px;
}
.register form .active p{
	display: inline;
	width: 100px;
	font-size: 15px;
}
.register form .active input{
	width: 300px;
	height: 25px;
	border-radius: 10px;
	background-color: #fff3ee;
	border: 1px solid red;
	outline: none;
	padding-left: 10px;
}
.register form .act input{
	text-align: center;
	margin-left: 150px;
	width: 200px;
	height: 25px;
	background-color: #FF6700;
	color: #fff;
	border-radius: 10px;
	border: 0;
	outline: none;
}